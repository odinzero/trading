<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/beans  http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/util  http://www.springframework.org/schema/util/spring-util-3.1.xsd">  
 
    
    <bean id="waterMark_btc" class="java.io.File" >
        <constructor-arg type="java.lang.String" name="pathname" value="./src/main/resources/img/bitcoin.png" />
    </bean>
    
    <bean id="waterMark_dash" class="java.io.File" >
        <constructor-arg type="java.lang.String" name="pathname" value="./src/main/resources/img/dash.png" />
    </bean>
    
    <bean id="waterMark_ethereum" class="java.io.File" >
        <constructor-arg type="java.lang.String" name="pathname" value="./src/main/resources/img/ethereum.png" />
    </bean>
    
    <bean id="waterMark_tether" class="java.io.File" >
        <constructor-arg type="java.lang.String" name="pathname" value="./src/main/resources/img/tether.png" />
    </bean>
    
    <bean id="waterMark_xrp" class="java.io.File" >
        <constructor-arg type="java.lang.String" name="pathname" value="./src/main/resources/img/xrp.png" />
    </bean>
    
    <bean id="waterMark_usd" class="java.io.File" >
        <constructor-arg type="java.lang.String" name="pathname" value="./src/main/resources/img/USD.png" />
    </bean>
    
</beans>
